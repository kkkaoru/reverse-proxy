name = "reverse-proxy-queue"
main = "src/index.ts"
compatibility_date = "2025-10-09"
compatibility_flags = ["nodejs_compat"]
workers_dev = true

[browser]
binding = "BROWSER"

[[kv_namespaces]]
binding = "KV"
id = "${CLOUDFLARE_KV_NAMESPACE_ID}"

[vars]
API_DOMAIN = "${API_DOMAIN}"
# IP Rotate configuration (optional)
# Note: For production, consider using `wrangler secret put` for sensitive values
IP_ROTATE_ENDPOINTS = "${IP_ROTATE_ENDPOINTS}"
IP_ROTATE_AUTH_TYPE = "${IP_ROTATE_AUTH_TYPE}"
IP_ROTATE_API_KEY = "${IP_ROTATE_API_KEY}"
IP_ROTATE_AWS_ACCESS_KEY_ID = "${IP_ROTATE_AWS_ACCESS_KEY_ID}"
IP_ROTATE_AWS_SECRET_ACCESS_KEY = "${IP_ROTATE_AWS_SECRET_ACCESS_KEY}"
IP_ROTATE_AWS_REGION = "${IP_ROTATE_AWS_REGION}"

[observability.logs]
enabled = true
head_sampling_rate = 1
invocation_logs = true
